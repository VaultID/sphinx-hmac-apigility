{
    "name": "VaultID/sphinx-hmac-apigility",
    "description": "Hash-based Message Authentication Code (HMAC) for ZendFramework2",
    "license": "Apache-2.0",
    "keywords": [
        "hmac",
        "zf3",
        "apigility"
    ],
    "homepage": "https://github.com/VaultID/sphinx-hmac-apigility",
    "autoload": {
        "psr-4": {
            "RB\\Sphinx\\Hmac\\Zend\\": "./src/"
        },
        "classmap": [
            "./Module.php"
        ]
    },
    "repositories": [
        {
	    "type": "vcs",
	    "url": "https://github.com/reinaldoborges/rb-sphinx-hmac.git"
        }
    ],
    "require": {
        "php": ">=5.5",
        "rb/sphinx-hmac": "v1.0.0",
        "zendframework/zend-http": "~2.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-develop": "0.1-dev"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZendTest\\Http\\": "test/"
        },
        "classmap": [
            "./Module.php"
        ]
    },
    "require-dev": {
        "phpunit/PHPUnit": "~4.0"
    }
}
